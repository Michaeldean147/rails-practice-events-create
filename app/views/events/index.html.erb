<h1>HELLO</h1>

<%= link_to "Create A New Event", new_event_path %>

 <table>
   <thead>
     <tr>
       <th>
         Date
       </th>
       <th>
         Description
       </th>
       <th>
         Requires_id
       </th>
       <th>
         Capacity
       </th>
       <th>
         Directions
       </th>
     </tr>
   </thead>


  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td>
         <%= event.date %>
        </td>
        <td>
         <%= event.description %>
        </td>
        <td>
         <%= event.requires_id %>
        </td>
        <td>
         <%= event.capacity %>
        </td>
        <td>
         <%= event.directions %>
        </td>
      </tr>
    <% end %>
  </tbody>


</table>
